{
  "name": "r3f_particles",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .js,.jsx,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md}\"",
    "lint": "npm run lint:format && npm run lint:fix ",
    "test": "echo \"No tests\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-three/drei": "^9.40.0",
    "@react-three/fiber": "^8.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.146.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/three": "^0.144.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,md}": [
      "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md}\"",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
